{
  "compression": "GZ",
  "directories": [
    "templates"
  ],
  "files": [
    "LICENSE"
  ],
  "finder": [
    {
      "exclude": [
        "Tests"
      ],
      "in": "lib"
    },
    {
      "name": "{\\.[php|bash|fish|zsh]}",
      "exclude": [
        "tests",
        "test"
      ],
      "in": "vendor"
    }
  ],
  "git-tag": "git_tag",
  "intercept": true,
  "output": "build/phpactor.phar"
}
