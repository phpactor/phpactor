<?php

class Foo {
    function foo(Foo|Bar $foo)
    {
        wrAssertType('Foo|Bar', $foo, 'one');

        if (!$foo instanceof Bar) {

            wrAssertType('Foo', $foo);
            return;
        }

        wrAssertType('Foo&Bar', $foo, 'two');

        if ($foo instanceof Baz) {
            wrAssertType('Baz', $foo);
            return;
        }
    }
}
