{
    "name": "phpactor/phpactor",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:phpactor/code-transform"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpactor/class-mover"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpactor/code-builder"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phpactor/source-code-filesystem"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/console": "~2.7|^3.0@stable",
        "phpbench/container": "^1.0",
        "phpactor/class-mover": "dev-master",
        "phpactor/source-code-filesystem": "dev-master",
        "phpactor/class-to-file": "dev-master",
        "phpactor/code-transform": "dev-master",
        "phpactor/worse-reflection": "dev-master",
        "symfony/filesystem": "^3.3",
        "webmozart/glob": "^4.1",
        "dnoegel/php-xdg-base-dir": "^0.1.0",
        "symfony/yaml": "^3.3",
        "microsoft/tolerant-php-parser": "v0.0.4",
        "monolog/monolog": "^2.0@dev"
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "symfony/process": "^2.7|^3.0",
        "symfony/options-resolver": "^2.7|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Tests\\": "tests/"
        }
    }
}
